<%= form_for(@glossary, remote: true) do |f| %>
	<div class="glossary-input">
		<%= f.text_field :title, placeholder: "glossary" %>
	</div>
	<div class="invisi-button">
		<%= f.submit "Post", class: "btn btn-large btn-primary", id: "glossary-new-submit" %>
	</div>
	<div id="error_explanation"></div>
<% end %>